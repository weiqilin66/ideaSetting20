  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.iml;.idea;" />
    <extensionMap>
      <mapping pattern="persistent object 持久对象" type="AUTO_DETECTED" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping ext="setting" type="PLAIN_TEXT" />
      <removed_mapping ext="apk" approved="true" type="ARCHIVE" />
      <removed_mapping ext="gitignore" approved="true" type="GitIgnore file" />
      <removed_mapping ext="psd" approved="true" type="Image" />
      <removed_mapping pattern=".project" approved="true" type="Eclipse" />
    </extensionMap>
  </component>
</application>